{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,

        "**/*.js.map":  {"when": "$(basename).map"},
        "**/*.js": {"when": "$(basename).ts"},
        
        "node_modules": true,
    }
}